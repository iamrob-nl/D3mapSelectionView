<?xml version="1.0" encoding="utf-8" ?>
<widget id="D3mapSelectionView.widget.D3mapSelectionView" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>D3mapSelectionView</name>
    <description>The description of this widget.</description>

    <icon></icon>

    <properties>
        <property key="mfToExecute" type="microflow" required="true" defaultValue="">
          <caption>Microflow</caption>
          <category>Behavior</category>
          <description>The microflow to get the JSON structure, returned as a String.</description>
          <returnType type="String" />
        </property>

        <property key="colorArray" type="string" required="false">
          <caption>Country color</caption>
          <category>Appearance</category>
          <description>colors used on the map, Add more colors, comma separated.</description>
        </property>

        <property key="thresholdArray" type="string" required="false">
          <caption>color array</caption>
          <category>Appearance</category>
          <description>Threshold in items for showing the next color, comma separated.</description>
        </property>

        <!-- <property key="subject" type="string" required="true" defaultValue="Orders">
          <caption>Data subject</caption>
          <category>Appearance</category>
          <description>The name for the data list. By default "Orders", but could be anything.</description>
        </property> -->
    </properties>
</widget>
